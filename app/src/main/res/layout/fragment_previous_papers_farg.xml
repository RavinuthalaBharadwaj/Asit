<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lyt_previos_papers"
    android:background="@color/Homefragback"
    tools:context=".PreviousPapersFarg">


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottie"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_centerInParent="true"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_url="https://assets7.lottiefiles.com/packages/lf20_usmfx6bp.json"></com.airbnb.lottie.LottieAnimationView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Available soon"
        android:layout_below="@id/lottie"
        android:layout_centerInParent="true"
        android:textSize="17sp"
        android:fontFamily="@font/workmedium"
        android:layout_marginTop="15dp"
        android:textColor="@color/homefragfont"/>

</RelativeLayout>